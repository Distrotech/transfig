$(LIBTRANSFIG)::
	cd $(DEVDIR); $(MAKE) DEFINES="$(DEVDEFINES) $(DIR_DEFS)" libtransfig.a

depend::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;;--*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "depending" "in $(CURRENT_DIR)/$$i..."; \
		$(MAKE) -C $$i $(MFLAGS) $(PARALLELMFLAGS)  depend; \
	done

        PROGRAM = fig2dev.exe

all:: fig2dev.exe

fig2dev.exe: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: fig2dev.exe
	@if [ -d $(DESTDIR)$(BINDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(BINDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(BINDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  fig2dev.exe $(DESTDIR)$(BINDIR)/fig2dev.exe

all:: fig2dev.$(MANNEWSUFFIX)

fig2dev.$(MANNEWSUFFIX): fig2dev.$(MANSRCSUFFIX)
	$(RM) $@
	if test -z "$(PREPROCESSMANPAGES)" ; then \
	   cd `dirname fig2dev` && \
	   $(LN) `basename fig2dev.$(MANSRCSUFFIX)` `basename $@`; \
	else \
	      	     $(RAWCPP)  $(MANDEFS) $(EXTRAMANDEFS) \
	     < fig2dev.$(MANSRCSUFFIX) | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@; \
	fi

cleandir::
	$(RM) fig2dev.$(MANNEWSUFFIX)

install.man:: fig2dev.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(MANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(MANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(MANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) fig2dev.$(MANNEWSUFFIX) $(DESTDIR)$(MANDIR)/fig2dev.$(MANSUFFIX)

depend:: Makefile.dep

Makefile.dep::
	$(RM) $@
	$(DEPEND) -f- $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS) > $@

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

cleandir::
	$(RM) fig2dev.exe

